ow.addEventListener('load', onLoad);\r\n\r\nfunction noScroll(e) {\r\n    e.preventDefault();\r\n    window.scrollTo(0, 0);\r\n}\r\n\r\n// add listener to disable scroll\r\nwindow.addEventListener('scroll', noScroll);\r\n\r\nfunction onLoad(evt) {\r\n    preload();\r\n    const el = document.querySelector(\".loading-screen\");\r\n    const gsapFadeOutSelectors = [\r\n        document.querySelector('.Intro'),\r\n        document.querySelector('.Dash'),\r\n        document.querySelector('.TechStack'),\r\n        ...document.querySelectorAll('.Project'),\r\n        ...document.querySelectorAll('.GroupName'),\r\n        document.querySelector('.Artworks'),\r\n        document.querySelector('.GetInTouch')\r\n    ];\r\n\r\n    // const gsapSwoopSelectors = [ ...document.querySelectorAll(\"span\"),\r\n    // ...document.querySelectorAll(\"h1\"), ...document.querySelectorAll(\".Project\r\n    // .Gallery\"), ...document.querySelectorAll(\"p\") ];\r\n\r\n    const gsapSwoopSelectors = gsapFadeOutSelectors.map((val, idx, arr) => {\r\n\r\n        if (val && !val.classList.contains(\"Intro\")) {\r\n            return [...val.children];\r\n        }\r\n    })\r\n\r\n    //Dissapear\r\n    gsapFadeOutSelectors.forEach(each => {\r\n        gsap.to(each, {\r\n            autoAlpha: 0,\r\n            scrollTrigger: {\r\n                trigger: each,\r\n                start: '75% 25%',\r\n                scrub: true,\r\n                // end: '50%', markers: true\r\n            }\r\n        });\r\n    })\r\n\r\n    //Swoop in\r\n    gsapSwoopSelectors.forEach(each => {\r\n        gsap.from(each, {\r\n            autoAlpha: 0,\r\n            skewY: \"3deg\",\r\n            scrollTrigger: {\r\n                trigger: each,\r\n                // scrub: true, markers: true,\r\n                start: \"100px 90%\",\r\n                end: 0\r\n            },\r\n            duration: 0.4\r\n        })\r\n    })\r\n\r\n    //Hide loader\r\n    gsapIntroTimeline.to(el, {\r\n        autoAlpha: 0,\r\n        duration: 0.6\r\n    }, 0);\r\n\r\n      // Remove listener to re-enable scroll\r\n  window.removeEventListener('scroll', noScroll);\r\n  console.clear();\r\n  console.log(\"Nothing to see here, move on. (⌐■_■)\");\r\n}"},"lineCount":null}},"error":null,"hash":"9cbe161d1b6a84110f7a17e09772aab6","cacheData":{"env":{}}}